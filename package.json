    },
    {
      "type": "opencollective",
      "url": "https://opencollective.com/konva"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/lavrton"
    }
  ],
  "peerDependencies": {
    "konva": "npm:konva-node-tweak@^1.0.0",
